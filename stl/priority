#include<iostream>
#include<queue>
using namespace std;

int main(){
  
  //max heap
  priority_queue<int>  maxi;
  
  
  //min heap
  priority_queue<int,vector<int>,greater<int> > mini;
  
  
  maxi.push(1);
  maxi.push(2);
  maxi.push(3);
  maxi.push(4);
  maxi.push(9);
  maxi.push(6);  
  maxi.push(7);
  
  
  int l=maxi.size();
  cout<<"size is : "<<maxi.size()<<endl;
  cout<<"maxi is working  "<<maxi.top()<<endl;
  
  for(int i=0;i<l;i++){
      
      cout<<maxi.top()<<" ";
      maxi.pop();
      
  }
  cout<<endl<<endl;
    
    cout<<"it's mini time"<<endl;
    //for mini
    
  mini.push(1);
  mini.push(2);
  mini.push(3);
  mini.push(4);
  mini.push(9);
  mini.push(6);  
  mini.push(7);
  
  
  int m=mini.size();
  cout<<"size is : "<<mini.size()<<endl;
  cout<<"maxi is working  "<<mini.top()<<endl;
  
  for(int i=0;i<m;i++){
      
      cout<<mini.top()<<" ";
      mini.pop();
      
  }
  cout<<endl;
    
  
  
  
    return 0;
}

